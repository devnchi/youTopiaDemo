      <section class="containerSec">
        <section class="userTools">
          <section>
          <% if (user.local.email) { %>
            <section>
              <strong><h2>Welcome, @<%= user.local.username %>!</h2> </strong>
            </section>
            <section class="userAvatar">
              <img src="<%= user.local.avatar %>" alt="users custom avatar">
            </section>
            <section>
              <form action="/posts" method="POST">
                <input class="hideContent" type="text" value="@<%= user.local.username %>" readonly="true" name="username">
                <textarea name="msg" placeholder="What are your thoughts?"></textarea>
                <br>
                <select class="makeBlue" class="postLocation" name="location">
                  <option value="feed">FEED</option>
                  <option value="artAesthetic">ART & AESTHETIC</option>
                  <option value="businessEcon">BUSINESS & ECONOMY</option>
                  <option value="eduAcad">EDUCATION & ACADEMIA</option>
                  <option value="govtPolitics">GOVERNMENT & POLITICS</option>
                  <option value="mediaEntertainment">MEDIA & ENTERTAINMENT</option>
                  <option value="spiritAlt">SPIRITUALITY & ALT THOUGHT</option>
                  <option value="scienceTech">SCIENCE & TECHNOLOGY</option>
                </select>
                <br>
                <button class="makeGreen" type="submit">Submit</button>
              </form>
            </section>
            <% } %>
        </section>
      <% if (posts[0].location === "spiritAlt") { %>
        <section class="showApi">
          <div id = "box">
            <h1>Curious about your zodiac sign?</h1>
          </div>
          <h2>Enter your birthday</h2>
          <p> Month:<input type="number" id="month" min="1" max="12"></p>
          <p> Day:<input type="number" id="day" min="1" max="31"></p>
          <a id="styled" onclick="zodiac()" class="button"><button type="button" name="button">Find your zodiac sign!</button></a>
          <br>
          <br>
          <br>
          <br>
          <p id="photo"></p>
          <p id= "space"><span id="result"></span></p>
          <br>
          <p id="characteristic"></p>
        </section>
    <%  } %>

