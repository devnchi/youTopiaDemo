        <% for(var i=0; i<posts.length; i++) {%>
          <li class="postBox" method="GET">
            <span class="userName"><%= posts[i].username %></span>
            <span><%= posts[i].msg %></span>
            <span><%= posts[i].heartLike %></span>
            <span class="heartLike"><i class="fas fa-heart" aria-hidden="true"></i></span>
          </li>
        <% } %>
        </ul>
      </div>
    </section>

