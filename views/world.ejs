          </section>
          <section>
            <form class="" action="index.html" method="post">
              <input type="date" name="" value="" id="date">
              <button type="button" name="button" id="activate"><i class="fas fa-search"></i></button>
            </form>
          </section>
        </section>
        <section id="content">
          <img id="nasaPic" src="" alt="">
          <iframe id="nasaFrame" src="" allowfullscreen></iframe>
          <p id="picDescription"></p>
        </section>
      </section>
  <%  } %>
